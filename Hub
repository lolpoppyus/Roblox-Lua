                                                                ---------------------------- Cursed Islands Menu ----------------------------
local CIS = Window:Category("Cursed Islands")
local CI = CIS:Sector("Misc")
CI:Cheat("Checkbox", "Remove Water", function()
    while true do
        wait(.1)
        game.Workspace.Map.Classic.Water:Destroy()
        game.Workspace.Map.Classic.Water.TouchInterest:Destroy()
    end
end)
