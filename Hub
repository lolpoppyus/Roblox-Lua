                                                                ---------------------------- Da Hood Menu ----------------------------
local DAH = Window:Category("Da Hood")
local DA = DAH:Sector("Misc")

DA:Cheat("Button", "Money Drop", function()
	d=game.Players.LocalPlayer.Character.HumanoidRootPart.Position;for e,f in pairs(game.Workspace.Ignored.Drop:GetChildren())do if f.Name=="MoneyDrop"then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=f.Parent:FindFirstChild("MoneyDrop").CFrame;if(game.Players.LocalPlayer.Character.HumanoidRootPart.Position-f.Position).Magnitude<=50 then wait(.8)fireclickdetector(f:FindFirstChild("ClickDetector"))wait(0)end end end;
end)

DA:Cheat("Button", "Items", function()
    d=game.Players.LocalPlayer.Character.HumanoidRootPart.Position;for h,i in pairs(game.Workspace.Ignored.ItemsDrop:GetChildren())do if i.Name=="Part"then if i:FindFirstChild("[LockPicker]")or i:FindFirstChild("[Knife]")then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=i.CFrame;if(game.Players.LocalPlayer.Character.HumanoidRootPart.Position-i.Position).Magnitude<=50 then wait()end end end end;
end)

local DAA = DAH:Sector("Teleports")

DAA:Cheat("Button", "Sewers", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(171,-41,153)
end)

DAA:Cheat("Button", "Bank", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-421,22,-283)
end)

                                                                ---------------------------- Wild West Menu ----------------------------
local WES = Window:Category("Wild West")
local WIS = WES:Sector("Misc")

WIS:Cheat("Button", "No Recoil", function()
    -- Wild West No Recoil
    for a,b in next, require(game.ReplicatedStorage.Modules.Load.Character.Items.Types.GunItem) do
        if a == "CalculateRecoil" then
            hookfunction(b, function()
                return 0
            end)
        end
    end
end)

WIS:Cheat("Button", "Inf Ammo", function()
    for a,b in next, require(game.ReplicatedStorage.Modules.Load.Character.Items.Types.GunItem) do
        if a == "Shoot" then
            local old; old = hookfunction(b, function(gun)
                if gun.Ammo then
                    gun.Ammo = math.huge
                else
                    print("cant find ammo table")
                end
                return old(gun)
            end)
        end
    end
end)
WIS:Cheat("Button", "Bypass All", function()
    game:GetService("ReplicatedStorage").Communication.Events:Remove()
end)
                                                                ---------------------------- Misc Menu ----------------------------
local MIS = Window:Category("Misc")
local MS = MIS:Sector("Misc")

MS:Cheat("Button", "FE Check", function()
    if game.Workspace.FilteringEnabled == true then
        local s = Instance.new("ScreenGui")
        s.Parent = game.Players.LocalPlayer.PlayerGui
        s.Name = "Message"
        wait()
        local t = Instance.new("TextLabel")
        t.Parent = s
        t.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
        t.TextColor3 = Color3.fromRGB(255, 255, 255)
        t.TextScaled = true
        t.BorderSizePixel = 3
        t.BorderColor3 = Color3.fromRGB(255, 0, 4)
        t.Position = UDim2.new(-0.192, 0,0.484, 0)
        t.BackgroundTransparency = 0.3
        t.Size = UDim2.new(0, 200,0, 50)
        t.Text = "Filtering is enabled."
        t:TweenPosition(UDim2.new(0, 0,0.485, 0), "In", "Bounce", 1.5)
        wait(2.5)
        t:TweenPosition(UDim2.new(-0.192, 0,0.484, 0), "Out", "Bounce", 1.5)
        wait(1.5)
        s:Destroy()
    else
        local a = Instance.new("ScreenGui")
        a.Parent = game.Players.LocalPlayer.PlayerGui
        a.Name = "Message"
        wait()
        local b = Instance.new("TextLabel")
        b.Parent = a
        b.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
        b.TextColor3 = Color3.fromRGB(255, 255, 255)
        b.TextScaled = true
        b.BorderSizePixel = 3
        b.BorderColor3 = Color3.fromRGB(0, 255, 42)
        b.Position = UDim2.new(-0.192, 0,0.484, 0)
        b.BackgroundTransparency = 0.3
        b.Size = UDim2.new(0, 200,0, 50)
        b.Text = "Filtering is disabled."
        b:TweenPosition(UDim2.new(0, 0,0.485, 0), "In", "Bounce", 1.5)
        wait(2.5)
        b:TweenPosition(UDim2.new(-0.192, 0,0.484, 0), "Out", "Bounce", 1.5)
        wait(1.5)
        a:Destroy()
    end
end)
