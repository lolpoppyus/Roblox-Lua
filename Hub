local wrld1 = PIG:Sector("World 1")

wrld1:Cheat("Button", "Red Key", function()
	local weeb = game.Workspace['ItemFolder']
        local uwu = true
        while uwu do
            wait()
            for _,v in ipairs(weeb:GetChildren()) do
                if v:FindFirstChild("Key") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame + Vector3.new(-5,0,0)
            end
        end
    end
end)
                                                                ---------------------------- Isle Menu ----------------------------
local ByP = Window:Category("Isle")
local BP = ByP:Sector("Bypaser")

BP:Cheat("Button", "Speed Bypass", function()
	if not getgenv().MTAPIMutex then loadstring(game:HttpGet("https://pastebin.com/raw/UwFCVrhS", true))() end
    game.Players.LocalPlayer.Character.Humanoid:AddPropertyEmulator("WalkSpeed")
end)

BP:Cheat("Button", "Jump Bypass", function()
	if not getgenv().MTAPIMutex then loadstring(game:HttpGet("https://pastebin.com/raw/UwFCVrhS", true))() end
    game.Players.LocalPlayer.Character.Humanoid:AddPropertyEmulator("JumpPower")
end)

BP:Cheat("Button", "BYPASS SERVICES", function()
	workspace.Services:Remove()
end)
