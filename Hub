local Pop = loadstring(game:HttpGet("http://finity.vip/scripts/finity_lib.lua"))()

local Window = Pop.new(true)
Window.ChangeToggleKey(Enum.KeyCode.Semicolon)
                                                                ---------------------------- Player Mod Menu ----------------------------
local Plr = Window:Category("Player Mods")
local Player = Plr:Sector("Local Cheats")
                -- Movement Speed --
Player:Cheat("Textbox", "WalkSpeed", function(Value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(Value) or 16
end
)
                -- Jump Power --
Player:Cheat("Textbox", "JumpPower", function(Value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = tonumber(Value) or 50
end
)
                -- NoClip --
Player:Cheat("Checkbox", "NoClip", function(gg)
    local arg = ( gg and true or false )
    if gg then
      _G.endme = game:GetService("RunService").RenderStepped:Connect(function()
        game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
      end)
    elseif not gg then
      game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(18)
      _G.endme:Disconnect()
    end
end)
                -- Fly --
local toggle = false
Player:Cheat("Checkbox", "Fly", function()
    toggle = not toggle
    local plyr = game.Players.LocalPlayer
    local char = plyr.Character
    local Torso = char:WaitForChild("LowerTorso")
    local Mouse = plyr:GetMouse()
    if toggle then
        local BV = Instance.new("BodyVelocity",Torso)
        BV.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        while toggle and wait() do
            BV.Velocity = Mouse.Hit.lookVector*100
            wait()
        end
    end
    if not toggle then
        Torso:FindFirstChildOfClass("BodyVelocity"):Remove()
    end
end)

Player:Cheat("Checkbox", "Inf Jump", function(gg)
    local InfiniteJumpEnabled = true
    local arg = ( gg and true or false )
    if gg then
      _G.infj = game:GetService("UserInputService").JumpRequest:connect(function()
            if InfiniteJumpEnabled then
            game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
        end
    end)
    elseif not gg then
      _G.infj:Disconnect()
    end
end)
local vu = game:GetService("VirtualUser")
Player:Cheat("Button", "Anti AFK", function()
	game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
     end)
end)

local tele = Plr:Sector("Teleport")
tele:Cheat("Textbox", "Teleport To", function(Value)
    print("Teleported to:", Value)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.AWholeClxwn.Character.HumanoidRootPart.CFrame
end, {
	placeholder = "Player Name"
})

                -- ESP --
local ESP = Plr:Sector("ESP")
ESP:Cheat("Checkbox", "Player ESP", function()
    toggle = not toggle
    if toggle then
        for __,v in pairs(game.Players:GetChildren()) do
            if v.Name == game.Players.LocalPlayer.Name then
                print("HOLDER")
                else
                local a = Instance.new("BillboardGui",v.Character.HumanoidRootPart)
                a.Size = UDim2.new(8,0, 8,0)
                a.AlwaysOnTop = true    
                local b = Instance.new("Frame",a)
                b.Size = UDim2.new(1,0, 1,0)  
                b.BackgroundTransparency = 0.75
                b.BorderSizePixel = 0
                b.BackgroundColor3 = Color3.new(0, 17, 255)
            end
        end
    end
    if not toggle then

    end
end)
                                                                ---------------------------- Murder Menu ----------------------------
local MW = Window:Category("Murder")
local MD = MW:Sector("Murder Game")

MD:Cheat("Checkbox", "Check Muderer", function()
    local plrs = game.Players:GetChildren() 
    for i,v in pairs(plrs) do
        if v.Status.Role.Value == "Murderer" then
            while true do
                print(v.Status.FakeName.Value .. " is murderer!")
                wait(10)
            end
        end
    end
end)

